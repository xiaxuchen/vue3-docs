#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xuchen.xia/FrontProjects/vue3-docs/node_modules/.pnpm/vuepress@1.9.10_@vue+compiler-sfc@3.5.13_hogan.js@3.0.2_lodash@4.17.21/node_modules/vuepress/node_modules:/Users/xuchen.xia/FrontProjects/vue3-docs/node_modules/.pnpm/vuepress@1.9.10_@vue+compiler-sfc@3.5.13_hogan.js@3.0.2_lodash@4.17.21/node_modules:/Users/xuchen.xia/FrontProjects/vue3-docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xuchen.xia/FrontProjects/vue3-docs/node_modules/.pnpm/vuepress@1.9.10_@vue+compiler-sfc@3.5.13_hogan.js@3.0.2_lodash@4.17.21/node_modules/vuepress/node_modules:/Users/xuchen.xia/FrontProjects/vue3-docs/node_modules/.pnpm/vuepress@1.9.10_@vue+compiler-sfc@3.5.13_hogan.js@3.0.2_lodash@4.17.21/node_modules:/Users/xuchen.xia/FrontProjects/vue3-docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vuepress/cli.js" "$@"
else
  exec node  "$basedir/../vuepress/cli.js" "$@"
fi
